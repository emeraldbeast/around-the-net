  <script>
    const btn = document.getElementById('hamburger');
    const menu = document.getElementById('navLinks');
    const links = menu.querySelectorAll('a');
    btn.addEventListener('click', () => { menu.classList.toggle('hidden'); });
    links.forEach(link => {
      link.addEventListener('click', () => {
        if (window.innerWidth < 768) { menu.classList.add('hidden'); }
      });
    });

    document.addEventListener('DOMContentLoaded', () => {
      const articles = Array.from(document.querySelectorAll('[id^="article-"]'))
        .filter(el => /^article-\d+$/.test(el.id));

      if (articles.length === 0) return;

      const parent = articles[0].parentElement;

      articles.sort((a, b) => {
        const numA = parseInt(a.id.split('-')[1], 10);
        const numB = parseInt(b.id.split('-')[1], 10);
        return numB - numA;
      });

      articles.forEach(el => parent.appendChild(el));
    });

    const toggleButtons = document.querySelectorAll('.toggle-btn');
    const toggleContents = document.querySelectorAll('.toggle-content');

    toggleButtons.forEach((btn, idx) => {
      btn.addEventListener('click', () => {
        toggleContents.forEach((content, i) => {
          content.classList.toggle('hidden', i !== idx);
        });
      });
    });

    const lastIndex = toggleContents.length - 1;
    toggleContents.forEach((content, i) => {
      content.classList.toggle('hidden', i !== lastIndex);
    });
  </script>
