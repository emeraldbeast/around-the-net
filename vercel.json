{
  "version": 2,
  "routes": [
    {
      "src": "/animations/(.*)",
      "dest": "/animations/$1"
    },
    {
      "src": "/logos/(.*)",
      "dest": "/logos/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ]
}
